<IfModule mod_rewrite.c>
    RewriteEngine On

    # Não redireciona se for um arquivo (css, js, img) ou diretório existente
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    # Envia todas as outras requisições para index.php
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
</IfModule>